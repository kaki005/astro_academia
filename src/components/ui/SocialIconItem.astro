---
import { Icon } from "@/components/ui/icon";
import { LinkedIcon } from "../../types/icon";
interface Props {
	item: LinkedIcon;
}
const { item } = Astro.props;
const { class: className = "" } = Astro.props;
---

<i
	class=`${item.icon_id} ${className} text-${item.color != null ? "[" + item.color + "]" : "black"}`
></i>
